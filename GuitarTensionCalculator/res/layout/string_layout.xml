<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/buttonFrequency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="Frequency" />

    <TextView
        android:id="@+id/buttonGauge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="Gauge    " />

    <TextView
        android:id="@+id/buttonNote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Note     " />

    <TextView
        android:id="@+id/buttonStringType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/buttonFrequency"
        android:layout_marginTop="26dp"
        android:text="Frequency" />

    <TextView
        android:id="@+id/buttonTension"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/buttonStringType"
        android:layout_alignBottom="@+id/buttonStringType"
        android:layout_alignRight="@+id/buttonNote"
        android:text="Frequency" />

    <TextView
        android:id="@+id/buttonOctave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/buttonTension"
        android:layout_alignBottom="@+id/buttonTension"
        android:layout_alignParentRight="true"
        android:text="Tension" />

</RelativeLayout>